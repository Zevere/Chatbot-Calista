{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "entry",
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-pipeline-operator",
            {
                "proposal": "minimal"
            }
        ],
        [
            "@babel/plugin-proposal-nullish-coalescing-operator"
        ],
        [
            "@babel/plugin-transform-flow-strip-types"
        ]
    ]
}