{
    "parser": "babel-eslint",
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2018,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "plugins": [
        "babel"
    ],
    "env": {
        "browser": false,
        "node": true
    },
    "rules": {
        "strict": "off"
    }
}