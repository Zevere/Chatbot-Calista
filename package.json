{
  "name": "calista",
  "version": "0.1.0",
  "description": "Add some zeverity to your Slack.",
  "main": "bin/www",
  "scripts": {
    "start": "npm run build:release && node dist/release",
    "start:debug": "npm run build:debug && node dist/debug",
    "build:debug": "babel src --out-dir dist/debug --source-maps",
    "build:release": "webpack -p --config webpack.config.ts",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --config webpack.dev.config.ts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zevere/SlackBot-Calista.git"
  },
  "keywords": [
    "chatbot",
    "slack",
    "node"
  ],
  "author": "Joseph Harrison-Lim",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Zevere/SlackBot-Calista/issues"
  },
  "homepage": "https://github.com/Zevere/SlackBot-Calista#readme",
  "dependencies": {
    "@slack/client": "^4.8.0",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.5.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "flow-runtime": "^0.17.0",
    "http-errors": "^1.7.0",
    "mongoose": "^5.3.2",
    "morgan": "^1.9.0",
    "rotating-file-stream": "^1.3.7",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.2.19",
    "@types/webpack": "^4.4.13",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "babel-plugin-flow-runtime": "^0.17.0",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.2.0",
    "nodemon": "^1.18.4",
    "nodemon-webpack-plugin": "^4.0.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1"
  }
}
