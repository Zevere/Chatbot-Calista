{
    "plugins": ["node_modules/jsdoc-babel", "plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["src"],
        "exclude": ["node_modules"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "docs",
        "private": true,
        "recurse": true
    }

}